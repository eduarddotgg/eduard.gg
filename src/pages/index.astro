---
import Head from '../components/Head.astro';
import { GitHub, Twitter } from 'astro-feather-icons'

const { title, description } = Astro.props;
---

<html lang="en">
<head>
  <Head title={title} description={description}/>
</head>
<body>
<main class="relative">
  <div class="flex flex-col items-center gap-24 justify-center w-full min-h-screen">
    <h1 class="text-size-72 font-900 uppercase">Eduard Pochtar</h1>
    <div class="flex gap-24">
      <a href="https://github.com/eduarddotgg" rel="noopener" target="_blank">
        <GitHub/>
      </a>
      <a href="https://x.com/eduarddotgg" rel="noopener" target="_blank">
        <Twitter/>
      </a>
    </div>
  </div>
</main>
</body>
</html>
